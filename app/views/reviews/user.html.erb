<% @user.each do |r| %>
<ul>
  <li> <%= r.title %> </li>
  <li> <%= r.rating %> </li>
  <li> <%= r.description %> </li>
  <li> <%= r.created_at %> </li>
  <li> <%= r.book_title %> </li>
  <li> <%= r.cover_image  %> </li>
  <p> <%= link_to "Delete Review", book_review_path(r.book_id, r.id), method: :delete %> </p>
</ul>
<% end %>
